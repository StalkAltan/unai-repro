{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/test/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/test",
        "main": "libs/test/src/index.ts",
        "tsConfig": "libs/test/tsconfig.lib.json",
        "assets": [
          "libs/test/*.md",
          {
            "input": "./libs/test/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/test/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/test",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/test",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/test/**/*.ts",
          "libs/test/generators.json",
          "libs/test/executors.json",
          "libs/test/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/test"],
      "options": {
        "jestConfig": "libs/test/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
